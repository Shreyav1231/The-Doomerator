{
    "manifest_version": 2,
    "name": "The Doomerator",
    "version": "1.0",
    "description": "extension to help you manage doomscrolling on web applications",
    "permissions": ["storage", "activeTab"],
    "browser_action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["index.js"]
      }
    ]
  }
  